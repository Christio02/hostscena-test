{
  "name": "hostscena",
  "description": "Høstscena - Next.js + Sanity Festival Website",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:next": "npm run dev --workspace=frontend",
    "dev:studio": "npm run dev --workspace=studio",
    "build": "npm run build --workspace=frontend",
    "build:studio": "npm run build --workspace=studio",
    "deploy:studio": "npm run deploy --workspace=studio"
  },
  "keywords": [
    "Sanity",
    "Next.js",
    "Festival",
    "Høstscena"
  ],
  "workspaces": [
    "studio",
    "frontend"
  ],
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@rollup/wasm-node": "^4.43.0",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
